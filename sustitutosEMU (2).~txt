
SELECT DISTINCT t1.subst
FROM emusubstitution t1, emucodification t2
WHERE
t1.newsstand=t2.newsstand
AND t2.distributorcode IN
(
'00012','0375','0431','0500','0776','0932','1380','4640','4962','4977','4996'
);

